{
    "generators": {
        "department": [
            {
                "type": "dynamic",
                "generatorOperationId": "portal.Department.addDepartment",
                "dataPath": "$.response.body:$.id"
            }
        ],
        "contact": [
            {
                "type": "dynamic",
                "generatorOperationId": "portal.Contact.addContact",
                "dataPath": "$.response.body:$.id"
            }
        ],
        "agent": [
            {
                "type": "dynamic",
                "generatorOperationId": "portal.Agent.addAgent",
                "dataPath": "$.response.body:$.id"
            }
        ],
        "ticket": [
            {
                "type": "dynamic",
                "generatorOperationId": "portal.Ticket.createTicketAuthenticated",
                "dependencies": [
                    "department",
                    "contact",
                    "agent"
                ],
                "dataPath": "$.response.body:$.id"
            }
        ]
    }
}