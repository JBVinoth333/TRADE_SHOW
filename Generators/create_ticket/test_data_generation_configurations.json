{
    "generators": {
        "contact_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Contact.createContact",
                "dataPath": "$.response.body:$.id",
                "name": "contacts"
            }
        ],
        "department_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Department.createDepartment",
                "dataPath": "$.response.body:$.id",
                "name": "departments"
            }
        ],
        "ticket": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Ticket.createTicket",
                "dependencies": [
                    "contact_id",
                    "department_id"
                ],
                "dataPath": "$.response.body:$.id",
                "name": "tickets"
            }
        ],
        "created_ticket_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "created_ticket_subject": [
            {
                "type": "reference",
                "ref": "$.response.body:$.subject"
            }
        ],
        "created_ticket_status": [
            {
                "type": "reference",
                "ref": "$.response.body:$.status"
            }
        ]
    }
}