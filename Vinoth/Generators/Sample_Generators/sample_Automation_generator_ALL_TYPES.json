{
    "generators": {
        "automation_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Automation.getAutomations",
                "dataPath": "$.response.body:$.data[*].id"
            }
        ],
        "automation_name": [
            {
                "type": "static",
                "value": "Auto Assign Tickets"
            }
        ],
        "automation_status": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
                "inputs": {
                    "fieldName": "status",
                    "moduleName": "Automation",
                    "orgId": "$.input.query:$.orgId"
                }
            }
        ],
        "automation_trigger": [
            {
                "type": "reference",
                "ref": "$.input.body:$.triggerId"
            }
        ],
        "automation_is_enabled": [
            {
                "type": "conditional",
                "condition": "status == 'ACTIVE'",
                "then": {
                    "type": "static",
                    "value": true
                },
                "else": {
                    "type": "static",
                    "value": false
                }
            }
        ]
    }
}