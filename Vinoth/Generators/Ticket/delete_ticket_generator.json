{
  "generators": {
    "ticket_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Ticket.createTicket",
        "dataPath": "$.response.body:$.id",
        "name": "tickets"
      }
    ],
    "delete_ticket": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Ticket.moveToTrash",
        "params": {
          "ticketIds": ["{{ticket_id}}"]
        },
        "dataPath": "$.response.statusCode"
      }
    ]
  }
}
