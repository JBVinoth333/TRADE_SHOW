{
    "generators": {
        "available_book_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getBooks",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "AVAILABLE"
                }
            }
        ],
        "any_book_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getBooks",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "member_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getMembers",
                "dataPath": "$.response.body:$.data[0].id",
                "params": {
                    "status": "ACTIVE"
                }
            }
        ],
        "borrow_date": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "current",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "due_date": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "future",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "borrow_status": [
            {
                "type": "static",
                "value": "BORROWED"
            }
        ],
        "borrow_transaction_id": [
            {
                "type": "remote",
                "generatorMethod": "com.example.LibraryDataGenerator.generateUUID"
            }
        ],
        "request_book_id": [
            {
                "type": "reference",
                "ref": "$.input.body:$.bookId"
            }
        ],
        "request_member_id": [
            {
                "type": "reference",
                "ref": "$.input.body:$.memberId"
            }
        ],
        "path_param_book_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.bookId"
            }
        ],
        "query_member_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.memberId"
            }
        ]
    }
}