{
    "generators": {
        "request_ticket_subject": [
            {
                "type": "reference",
                "ref": "$.input.body:$.subject"
            }
        ],
        "request_ticket_description": [
            {
                "type": "reference",
                "ref": "$.input.body:$.description"
            }
        ],
        "request_ticket_priority": [
            {
                "type": "reference",
                "ref": "$.input.body:$.priority"
            }
        ],
        "created_ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "created_ticket_subject": [
            {
                "type": "reference",
                "ref": "$.response.body:$.subject"
            }
        ],
        "created_ticket_status": [
            {
                "type": "reference",
                "ref": "$.response.body:$.status"
            }
        ],
        "any_ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "open_ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "OPEN"
                }
            }
        ],
        "ticket_status": [
            {
                "type": "static",
                "value": ["OPEN", "ONHOLD", "CLOSED", "PENDING"]
            }
        ],
        "ticket_priority": [
            {
                "type": "static",
                "value": 2
            }
        ],
        "created_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "current",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "ticket_number": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateTicketNumber",
                "params": {
                    "prefix": "TKT"
                }
            }
        ],
        "invalid_ticket_id": [
            {
                "type": "static",
                "value": 99999
            }
        ],
        "path_param_ticket_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "query_ticket_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.id"
            }
        ]
    }
}
