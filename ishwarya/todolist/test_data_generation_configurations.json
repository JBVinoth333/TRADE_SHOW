{
    "generators": {
        "new_todo_list_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTodoLists",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "new_todo_list_title": [
            {
                "type": "static",
                "value": "My Tasks"
            }
        ],
        "new_todo_list_owner_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getUsers",
                "dataPath": "$.response.body:$.data[0].id",
                "params": {
                    "status": "ACTIVE"
                }
            }
        ],
        "created_todolist_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "request_todolist_title": [
            {
                "type": "reference",
                "ref": "$.input.body:$.title"
            }
        ],
        "request_todolist_owner_id": [
            {
                "type": "reference",
                "ref": "$.input.body:$.ownerId"
            }
        ],
        "created_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "current",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "todolist_status": [
            {
                "type": "static",
                "value": "ACTIVE"
            }
        ],
        "invalid_todolist_id": [
            {
                "type": "static",
                "value": 99999
            }
        ],
        "path_param_todolist_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "query_todolist_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.id"
            }
        ]
    }
}