{
    "generators": {
        "request_account_name": [
            {
                "type": "reference",
                "ref": "$.input.body:$.name"
            }
        ],
        "request_account_email": [
            {
                "type": "reference",
                "ref": "$.input.body:$.email"
            }
        ],
        "created_account_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "created_account_email": [
            {
                "type": "reference",
                "ref": "$.response.body:$.email"
            }
        ],
        "any_account_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getAccounts",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "active_account_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getAccounts",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "ACTIVE"
                }
            }
        ],
        "account_status": [
            {
                "type": "static",
                "value": "ACTIVE"
            }
        ],
        "created_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "current",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "account_number": [
            {
                "type": "remote",
                "generatorMethod": "com.example.AccountDataGenerator.generateAccountNumber"
            }
        ],
        "invalid_account_id": [
            {
                "type": "static",
                "value": 99999
            }
        ],
        "path_param_account_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "query_account_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.id"
            }
        ]
    }
}