{
    "generators": {
        "book_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getBooks",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "available_book_ids": [
            {
                "type": "dynamic",
                "generatorOperationId": "getBooks",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "available": true
                }
            }
        ],
        "borrowed_book_ids": [
            {
                "type": "dynamic",
                "generatorOperationId": "getBooks",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "borrowed": true
                }
            }
        ],
        "user_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getUsers",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "active_user_ids": [
            {
                "type": "dynamic",
                "generatorOperationId": "getUsers",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "ACTIVE"
                }
            }
        ],
        "book_title": [
            {
                "type": "static",
                "value": "The Great Gatsby"
            }
        ],
        "book_author": [
            {
                "type": "static",
                "value": "F. Scott Fitzgerald"
            }
        ],
        "borrow_status_active": [
            {
                "type": "static",
                "value": "BORROWED"
            }
        ],
        "borrow_status_returned": [
            {
                "type": "static",
                "value": "RETURNED"
            }
        ],
        "borrow_duration_days": [
            {
                "type": "static",
                "value": 14
            }
        ],
        "max_renewals": [
            {
                "type": "static",
                "value": 2
            }
        ],
        "is_overdue": [
            {
                "type": "static",
                "value": false
            }
        ],
        "created_borrow_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "created_borrow_user_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.userId"
            }
        ],
        "created_borrow_book_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.bookId"
            }
        ],
        "request_borrow_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "request_book_id": [
            {
                "type": "reference",
                "ref": "$.input.body:$.bookId"
            }
        ],
        "request_user_id": [
            {
                "type": "reference",
                "ref": "$.input.body:$.userId"
            }
        ],
        "generate_borrow_date": [
            {
                "type": "remote",
                "generatorMethod": "com.example.BookDataGenerator.generateBorrowDate"
            }
        ],
        "generate_due_date": [
            {
                "type": "remote",
                "generatorMethod": "com.example.BookDataGenerator.generateDueDate",
                "params": {
                    "days": "14"
                }
            }
        ],
        "generate_borrow_uuid": [
            {
                "type": "remote",
                "generatorMethod": "com.example.BookDataGenerator.generateUUID"
            }
        ],
        "generate_isbn": [
            {
                "type": "remote",
                "generatorMethod": "com.example.BookDataGenerator.generateISBN"
            }
        ]
    }
}