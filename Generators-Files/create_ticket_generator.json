{
    "generators": {
        "new_ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "new_ticket_title": [
            {
                "type": "static",
                "value": "User cannot reset password"
            }
        ],
        "new_ticket_priority": [
            {
                "type": "static",
                "value": 2
            }
        ],
        "new_ticket_status": [
            {
                "type": "static",
                "value": "OPEN"
            }
        ],
        "created_ticket_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "created_ticket_title": [
            {
                "type": "reference",
                "ref": "$.response.body:$.title"
            }
        ],
        "created_ticket_status": [
            {
                "type": "reference",
                "ref": "$.response.body:$.status"
            }
        ],
        "request_ticket_title": [
            {
                "type": "reference",
                "ref": "$.input.body:$.title"
            }
        ],
        "request_ticket_priority": [
            {
                "type": "reference",
                "ref": "$.input.body:$.priority"
            }
        ],
        "request_ticket_status": [
            {
                "type": "reference",
                "ref": "$.input.body:$.status"
            }
        ],
        "path_param_ticket_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "query_ticket_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.id"
            }
        ],
        "generate_uuid": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateUUID"
            }
        ],
        "generate_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateTimestamp"
            }
        ],
        "generate_ticket_number": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateTicketNumber",
                "params": {
                    "prefix": "TKT",
                    "min": "1000",
                    "max": "9999"
                }
            }
        ]
    }
}
