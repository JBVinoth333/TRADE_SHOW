{
    "generators": {
        "ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "open_ticket_ids": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "OPEN"
                }
            }
        ],
        "closed_ticket_ids": [
            {
                "type": "dynamic",
                "generatorOperationId": "getTickets",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "CLOSED"
                }
            }
        ],
        "user_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getUsers",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "active_user_ids": [
            {
                "type": "dynamic",
                "generatorOperationId": "getUsers",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "ACTIVE"
                }
            }
        ],
        "agent_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "getAgents",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "ticket_title": [
            {
                "type": "static",
                "value": "Unable to login to account"
            }
        ],
        "ticket_description": [
            {
                "type": "static",
                "value": "User is unable to login to their account. Error message received."
            }
        ],
        "ticket_status_open": [
            {
                "type": "static",
                "value": "OPEN"
            }
        ],
        "ticket_status_in_progress": [
            {
                "type": "static",
                "value": "IN_PROGRESS"
            }
        ],
        "ticket_status_closed": [
            {
                "type": "static",
                "value": "CLOSED"
            }
        ],
        "ticket_priority_high": [
            {
                "type": "static",
                "value": "HIGH"
            }
        ],
        "ticket_priority_medium": [
            {
                "type": "static",
                "value": "MEDIUM"
            }
        ],
        "ticket_priority_low": [
            {
                "type": "static",
                "value": "LOW"
            }
        ],
        "ticket_category": [
            {
                "type": "static",
                "value": "TECHNICAL_SUPPORT"
            }
        ],
        "sla_hours": [
            {
                "type": "static",
                "value": 24
            }
        ],
        "is_escalated": [
            {
                "type": "static",
                "value": false
            }
        ],
        "created_ticket_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "created_ticket_number": [
            {
                "type": "reference",
                "ref": "$.response.body:$.ticketNumber"
            }
        ],
        "created_ticket_status": [
            {
                "type": "reference",
                "ref": "$.response.body:$.status"
            }
        ],
        "created_ticket_assigned_user": [
            {
                "type": "reference",
                "ref": "$.response.body:$.assignedUserId"
            }
        ],
        "request_ticket_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "request_ticket_title": [
            {
                "type": "reference",
                "ref": "$.input.body:$.title"
            }
        ],
        "request_ticket_priority": [
            {
                "type": "reference",
                "ref": "$.input.body:$.priority"
            }
        ],
        "request_assigned_user": [
            {
                "type": "reference",
                "ref": "$.input.body:$.assignedUserId"
            }
        ],
        "query_user_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.userId"
            }
        ],
        "generate_ticket_number": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateTicketNumber"
            }
        ],
        "generate_created_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateTimestamp"
            }
        ],
        "generate_due_date": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateDueDate",
                "params": {
                    "hours": "24"
                }
            }
        ],
        "generate_ticket_uuid": [
            {
                "type": "remote",
                "generatorMethod": "com.example.TicketDataGenerator.generateUUID"
            }
        ]
    }
}