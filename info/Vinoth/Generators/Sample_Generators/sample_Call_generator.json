{
    "generators": {
        "call_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Call.getCalls",
                "dataPath": "$.response.body:$.data[*].id"
            }
        ],
        "call_duration": [
            {
                "type": "static",
                "value": 300
            }
        ],
        "call_status": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
                "inputs": {
                    "fieldName": "status",
                    "moduleName": "Call",
                    "orgId": "$.input.query:$.orgId"
                }
            }
        ],
        "call_agent_id": [
            {
                "type": "reference",
                "ref": "$.input.body:$.agentId"
            }
        ],
        "call_direction": [
            {
                "type": "conditional",
                "condition": "status == 'completed'",
                "then": {
                    "type": "static",
                    "value": "INBOUND"
                },
                "else": {
                    "type": "static",
                    "value": "OUTBOUND"
                }
            }
        ]
    }
}