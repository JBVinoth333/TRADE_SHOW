{
    "generators": {
        "ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Ticket.getTickets",
                "dataPath": "$.response.body:$.data[*].id"
            }
        ],
        "open_ticket_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Ticket.getTickets",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "OPEN"
                }
            }
        ],
        "ticket_priority": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
                "inputs": {
                    "fieldName": "priority",
                    "moduleName": "Ticket",
                    "orgId": "$.input.query:$.orgId"
                }
            }
        ],
        "ticket_status": [
            {
                "type": "static",
                "value": "OPEN"
            }
        ],
        "ticket_description": [
            {
                "type": "reference",
                "ref": "$.input.body:$.description"
            }
        ],
        "ticket_due_date": [
            {
                "type": "conditional",
                "condition": "priority == 'HIGH'",
                "then": {
                    "type": "remote",
                    "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                    "inputs": {
                        "timeLine": "current",
                        "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"
                    }
                },
                "else": {
                    "type": "remote",
                    "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                    "inputs": {
                        "timeLine": "future",
                        "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"
                    }
                }
            }
        ]
    }
}