{
    "generators": {
        "article_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Article.listAllArticles",
                "dataPath": "$.response.body:$.data[*].id"
            }
        ],
        "published_article_id": [
            {
                "type": "dynamic",
                "generatorOperationId": "support.Article.listAllArticles",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "published"
                }
            }
        ],
        "article_title": [
            {
                "type": "static",
                "value": "How to resolve common issues"
            }
        ],
        "article_status": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
                "inputs": {
                    "fieldName": "status",
                    "moduleName": "Article",
                    "orgId": "$.input.query:$.orgId"
                }
            }
        ],
        "article_category": [
            {
                "type": "reference",
                "ref": "$.input.body:$.categoryId"
            }
        ],
        "article_is_featured": [
            {
                "type": "conditional",
                "condition": "status == 'published'",
                "then": {
                    "type": "static",
                    "value": true
                },
                "else": {
                    "type": "static",
                    "value": false
                }
            }
        ]
    }
}