{
    "generators": {
        "request_ticket_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "request_ticket_subject": [
            {
                "type": "reference",
                "ref": "$.input.body:$.subject"
            }
        ],
        "request_ticket_description": [
            {
                "type": "reference",
                "ref": "$.input.body:$.description"
            }
        ],
        "request_ticket_priority": [
            {
                "type": "reference",
                "ref": "$.input.body:$.priority"
            }
        ],
        "request_ticket_status": [
            {
                "type": "reference",
                "ref": "$.input.body:$.status"
            }
        ],
        "updated_ticket_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "updated_ticket_subject": [
            {
                "type": "reference",
                "ref": "$.response.body:$.subject"
            }
        ],
        "updated_ticket_status": [
            {
                "type": "reference",
                "ref": "$.response.body:$.status"
            }
        ],
        "any_ticket_id": [
            {
                "type": "dynamic",
                "name": "ticket",
                "generatorOperationId": "support.Ticket.getTickets",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "open_ticket_id": [
            {
                "type": "dynamic",
                "name": "ticket",
                "generatorOperationId": "support.Ticket.getTickets",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "OPEN"
                }
            }
        ],
        "ticket_status": [
            {
                "type": "static",
                "value": [
                    "OPEN",
                    "ONHOLD",
                    "CLOSED",
                    "PENDING"
                ]
            }
        ],
        "ticket_priority": [
            {
                "type": "static",
                "value": [
                    2
                ]
            }
        ],
        "updated_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "current",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "invalid_ticket_id": [
            {
                "type": "static",
                "value": [
                    99999
                ]
            }
        ]
    }
}