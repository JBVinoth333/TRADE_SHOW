{
    "generators": {
        "request_user_name": [
            {
                "type": "reference",
                "ref": "$.input.body:$.name"
            }
        ],
        "request_user_username": [
            {
                "type": "reference",
                "ref": "$.input.body:$.username"
            }
        ],
        "request_user_email": [
            {
                "type": "reference",
                "ref": "$.input.body:$.email"
            }
        ],
        "created_user_id": [
            {
                "type": "reference",
                "ref": "$.response.body:$.id"
            }
        ],
        "created_user_email": [
            {
                "type": "reference",
                "ref": "$.response.body:$.email"
            }
        ],
        "any_user_id": [
            {
                "type": "dynamic",
                "name": "user",
                "generatorOperationId": "support.User.getUsers",
                "dataPath": "$.response.body:$.data[0].id"
            }
        ],
        "active_user_id": [
            {
                "type": "dynamic",
                "name": "user",
                "generatorOperationId": "support.User.getUsers",
                "dataPath": "$.response.body:$.data[*].id",
                "params": {
                    "status": "ACTIVE"
                }
            }
        ],
        "user_status": [
            {
                "type": "static",
                "value": [
                    "ACTIVE"
                ]
            }
        ],
        "created_timestamp": [
            {
                "type": "remote",
                "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
                "inputs": {
                    "timeLine": "current",
                    "format": "yyyy-MM-dd'T'HH:mm:ss'Z'"
                }
            }
        ],
        "invalid_user_id": [
            {
                "type": "static",
                "value": [
                    99999
                ]
            }
        ],
        "path_param_user_id": [
            {
                "type": "reference",
                "ref": "$.input.path:$.id"
            }
        ],
        "query_user_id": [
            {
                "type": "reference",
                "ref": "$.input.query:$.id"
            }
        ]
    }
}
